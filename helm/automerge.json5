{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  description: "automerge helm datasource non major versions",
  packageRules: [
    {
      matchDatasources: ["helm"],
      matchUpdateTypes: ["minor", "patch", "digest"],
      automerge: true,
      automergeType: "pr",
    },
  ],
}
